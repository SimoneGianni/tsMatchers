{
    "name": "tsmatchers",
    "version": "3.0.6",
    "devDependencies": {
        "@types/jest": "^24.9.0",
        "jest": "^24.9.0",
        "ts-jest": "^24.3.0",
        "typescript": "^3.7.5"
    },
    "scripts": {
        "build": "tsc",
        "test": "jest"
    },
    "license": "ASL 2.0",
    "repository": {
        "type": "git",
        "url": "git@github.com:SimoneGianni/tsMatchers.git"
    },
    "main": "js/index.js",
    "typings": "js/index.d.ts",
    "files": [
        "js/**/*"
    ],
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "globals": {
            "ts-jest": {
                "diagnostics": false
            }
        },
        "testMatch": [
            "**/__tests__/*.(ts|tsx|js)"
        ]
    }
}
